{
	"name": "Stackoverflow shortcut",
	"description": "Navigate your questions to Stackoverflow quickly",
	"version": "1.0.0",
	"manifest_version": 3,
	"background":{
		"service_worker": "background.js"
	},
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "./images/icon.png",
			"32": "./images/icon.png",
			"48": "./images/icon.png",
			"128": "./images/icon.png"
		}
	},
	"icons":{
		"16": "./images/icon.png",
		"32": "./images/icon.png",
		"48": "./images/icon.png",
		"128": "./images/icon.png"
	},
	"permissions":["tabs","scripting"]

}